# Scalebox SDK Configuration
# Copy this file to .env and fill in your values

# API Key (required)
SBX_API_KEY=your_api_key_here

# Alternative API Key (for compatibility)
E2B_API_KEY=your_api_key_here

# Access Token (optional, alternative to API key)
SBX_ACCESS_TOKEN=your_access_token_here

# API URL (optional, defaults to https://api.scalebox.dev)
SBX_API_URL=https://api.scalebox.dev

# Custom domain (optional)
SBX_DOMAIN=your_domain_here

# Debug mode (optional)
SBX_DEBUG=false

# Request timeout in milliseconds (optional, defaults to 30000)
SBX_REQUEST_TIMEOUT_MS=30000

# Sandbox timeout in milliseconds (optional, defaults to 300000)
SBX_SANDBOX_TIMEOUT_MS=300000
